{
    "extends": "fastify-tsconfig",
    "compilerOptions": {
      "rootDir": "./",
      "baseUrl": "./",
      "allowJs": true,
      "paths": {"@/*": ["src/*"]},
      "noUnusedLocals": false,
      "skipLibCheck": true,
      "resolveJsonModule": true,
    },
    "include": ["src/**/*.ts", "./*.ts"],
    "exclude": ["node_modules", "dist"]
}